# FLASH é usado para geração rápida de prompts, mutações, etc.
FLASH_API_KEY=dummy_key_for_ollama
FLASH_MODEL=ollama/qwen3:30b-a3b

# EVALUATION é usado para avaliar código gerado
EVALUATION_API_KEY=dummy_key_for_ollama
EVALUATION_MODEL=ollama/qwen3:30b-a3b

# BASE URLS
FLASH_BASE_URL=http://localhost:11434
EVALUATION_BASE_URL=http://localhost:11434

# --- LiteLLM Configuration ---
# Default model string for litellm (e.g., "gpt-3.5-turbo", "ollama/mistral", "claude-3-haiku-20240307")
# See litellm documentation for more model strings: https://docs.litellm.ai/docs/providers
LITELLM_DEFAULT_MODEL="gemini/gemini-2.0-flash-lite"
GEMINI_API_KEY=""

# As chaves abaixo são só necessárias se estiver usando outros provedores como OpenAI, Anthropic, etc.
# OPENAI_API_KEY=your_openai_key
# GOOGLE_API_KEY=your_google_key

# --- Optional Parameters ---
LITELLM_MAX_TOKENS=4096
LITELLM_TEMPERATURE=0.7
LITELLM_TOP_P=0.9
LITELLM_TOP_K=40